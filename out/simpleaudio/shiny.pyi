from typing import Any

class WaveObject:
    audio_data: Any
    num_channels: Any
    bytes_per_sample: Any
    sample_rate: Any
    def __init__(self, audio_data, num_channels: int = ..., bytes_per_sample: int = ..., sample_rate: int = ...) -> None: ...
    def play(self): ...
    @classmethod
    def from_wave_file(cls, wave_file): ...
    @classmethod
    def from_wave_read(cls, wave_read): ...

class PlayObject:
    play_id: Any
    def __init__(self, play_id) -> None: ...
    def stop(self) -> None: ...
    def wait_done(self) -> None: ...
    def is_playing(self): ...

def stop_all() -> None: ...
def play_buffer(audio_data, num_channels, bytes_per_sample, sample_rate): ...
